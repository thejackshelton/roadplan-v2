#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/jackshelton/dev/playground/demo-roadplan/node_modules/.pnpm/@qwik.dev+core@2.0.0-alpha.1_prettier@3.3.3_vite@5.4.11_@types+node@20.17.6_sass@1.81.0_/node_modules/@qwik.dev/core/node_modules:/Users/jackshelton/dev/playground/demo-roadplan/node_modules/.pnpm/@qwik.dev+core@2.0.0-alpha.1_prettier@3.3.3_vite@5.4.11_@types+node@20.17.6_sass@1.81.0_/node_modules/@qwik.dev/node_modules:/Users/jackshelton/dev/playground/demo-roadplan/node_modules/.pnpm/@qwik.dev+core@2.0.0-alpha.1_prettier@3.3.3_vite@5.4.11_@types+node@20.17.6_sass@1.81.0_/node_modules:/Users/jackshelton/dev/playground/demo-roadplan/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/jackshelton/dev/playground/demo-roadplan/node_modules/.pnpm/@qwik.dev+core@2.0.0-alpha.1_prettier@3.3.3_vite@5.4.11_@types+node@20.17.6_sass@1.81.0_/node_modules/@qwik.dev/core/node_modules:/Users/jackshelton/dev/playground/demo-roadplan/node_modules/.pnpm/@qwik.dev+core@2.0.0-alpha.1_prettier@3.3.3_vite@5.4.11_@types+node@20.17.6_sass@1.81.0_/node_modules/@qwik.dev/node_modules:/Users/jackshelton/dev/playground/demo-roadplan/node_modules/.pnpm/@qwik.dev+core@2.0.0-alpha.1_prettier@3.3.3_vite@5.4.11_@types+node@20.17.6_sass@1.81.0_/node_modules:/Users/jackshelton/dev/playground/demo-roadplan/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@qwik.dev/core/qwik-cli.cjs" "$@"
else
  exec node  "$basedir/../@qwik.dev/core/qwik-cli.cjs" "$@"
fi
